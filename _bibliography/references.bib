	number = {3},
	urldate = {2022-03-22},
	journal = {IEEE Transactions on Circuits and Systems I: Regular Papers},
	author = {Yang, Yang and Qian, Yue and Yue, Wenbin},
	month = mar,
	year = {2022},
	pages = {1360--1371},
	file = {Yang et al_2022_A Secure Dynamic Event-Triggered Mechanism for Resilient Control of Multi-Agent.pdf:C\:\\OneDrive - Universidad Complutense de Madrid (UCM)\\Zotero\\PhD\\Papers\\Hierarchical Multiagent Formation\\Protocol\\Off topic\\Yang et al_2022_A Secure Dynamic Event-Triggered Mechanism for Resilient Control of Multi-Agent.pdf:application/pdf},
}

@article{yang_state_2021,
