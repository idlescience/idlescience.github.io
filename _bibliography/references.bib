@article{yang_secure_2022,
	title = {A {Secure} {Dynamic} {Event}-{Triggered} {Mechanism} for {Resilient} {Control} of {Multi}-{Agent} {Systems} {Under} {Sensor} and {Actuator} {Attacks}},
