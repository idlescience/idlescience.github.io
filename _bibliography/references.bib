}

@inproceedings{michalak_representing_2009,
	address = {Stanford, California, USA},
	title = {On representing coalitional games with externalities},
	isbn = {978-1-60558-458-4},
	url = {http://portal.acm.org/citation.cfm?doid=1566374.1566377},
	doi = {10.1145/1566374.1566377},
	abstract = {We consider the issue of representing coalitional games in multiagent systems with externalities (i.e., in systems where the performance of one coalition may be affected by other co-existing coalitions). In addition to the conventional partition function game representation (P F G), we propose a number of new representations based on a new notion of externalities. In contrast to conventional game theory, our new concept is not related to the process by which the coalitions are formed, but rather to the effect that each coalition may have on the entire system and vice versa. We show that the new representations are fully expressive and, for many classes of games, more concise than the conventional P F G. Building upon these new representations, we propose a number of approaches to solve the coalition structure generation problem in systems with externalities. We show that, if externalities are characterised by various degrees of regularity, the new representations allow us to adapt coalition structure generation algorithms that were originally designed for domains with no externalities, so that they can be used when externalities are present. Finally, building upon [16] and [9], we present a uniÔ¨Åed method to solve the coalition structure generation problem in any system, with or without externalities, provided sufÔ¨Åcient information is available.},
	language = {en},
	urldate = {2022-12-05},
	booktitle = {Proceedings of the tenth {ACM} conference on {Electronic} commerce - {EC} '09},
	publisher = {ACM Press},
	author = {Michalak, Tomasz P. and Rahwan, Talal and Sroka, Jacek and Dowell, Andrew and Wooldridge, Michael J. and McBurney, Peter J. and Jennings, Nicholas R.},
	year = {2009},
	pages = {11},
	file = {Michalak et al_2009_On representing coalitional games with externalities.pdf:C\:\\OneDrive - Universidad Complutense de Madrid (UCM)\\Zotero\\PhD\\Papers\\Cooperative Games Visualizations\\Related Work\\Michalak et al_2009_On representing coalitional games with externalities.pdf:application/pdf},
