	note = {arXiv: 1909.04224},
	file = {Chen et al_2019_Signal Instructed Coordination in Cooperative Multi-agent Reinforcement Learning.pdf:C\:\\OneDrive - Universidad Complutense de Madrid (UCM)\\Zotero\\PhD\\Papers\\Hierarchical Multiagent Formation\\Protocol\\Core\\Chen et al_2019_Signal Instructed Coordination in Cooperative Multi-agent Reinforcement Learning.pdf:application/pdf},
}

