	note = {arXiv: 1812.08434},
	file = {Zhou et al_2019_Graph Neural Networks.pdf:C\:\\OneDrive - Universidad Complutense de Madrid (UCM)\\Zotero\\Transversal\\GNN\\Zhou et al_2019_Graph Neural Networks.pdf:application/pdf},
}

