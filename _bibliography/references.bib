@article{heilmeier_application_2020,
	title = {Application of {Monte} {Carlo} {Methods} to {Consider} {Probabilistic} {Effects} in a {Race} {Simulation} for {Circuit} {Motorsport}},
	volume = {10},
	issn = {2076-3417},
	url = {https://www.mdpi.com/2076-3417/10/12/4229},
	doi = {10.3390/app10124229},
	abstract = {Applying an optimal race strategy is a decisive factor in achieving the best possible result in a motorsport race. This mainly implies timing the pit stops perfectly and choosing the optimal tire compounds. Strategy engineers use race simulations to assess the effects of different strategic decisions (e.g., early vs. late pit stop) on the race result before and during a race. However, in reality, races rarely run as planned and are often decided by random events, for example, accidents that cause safety car phases. Besides, the course of a race is affected by many smaller probabilistic inÔ¨Çuences, for example, variability in the lap times. Consequently, these events and inÔ¨Çuences should be modeled within the race simulation if real races are to be simulated, and a robust race strategy is to be determined. Therefore, this paper presents how state of the art and new approaches can be combined to modeling the most important probabilistic inÔ¨Çuences on motorsport races‚Äîaccidents and failures, full course yellow and safety car phases, the drivers‚Äô starting performance, and variability in lap times and pit stop durations. The modeling is done using customized probability distributions as well as a novel ‚Äúghost‚Äù car approach, which allows the realistic consideration of the effect of safety cars within the race simulation. The interaction of all inÔ¨Çuences is evaluated based on the Monte Carlo method. The results demonstrate the validity of the models and show how Monte Carlo simulation enables assessing the robustness of race strategies. Knowing the robustness improves the basis for a reasonable determination of race strategies by strategy engineers.},
	language = {en},
	number = {12},
	urldate = {2023-03-27},
	journal = {Applied Sciences},
	author = {Heilmeier, Alexander and Graf, Michael and Betz, Johannes and Lienkamp, Markus},
	month = jun,
	year = {2020},
	pages = {4229},
	file = {Heilmeier et al_2020_Application of Monte Carlo Methods to Consider Probabilistic Effects in a Race.pdf:C\:\\OneDrive - Universidad Complutense de Madrid (UCM)\\Zotero\\Transversal\\F1\\Heilmeier et al_2020_Application of Monte Carlo Methods to Consider Probabilistic Effects in a Race.pdf:application/pdf},
