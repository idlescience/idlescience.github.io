}

@article{voice_graph_nodate,
	title = {Graph {Coalition} {Structure} {Generation}},
	abstract = {We give the Ô¨Årst analysis of the computational complexity of coalition structure generation over graphs. Given an undirected graph G = (N, E) and a valuation function v : 2N ‚Üí R over the subsets of nodes, the problem is to Ô¨Ånd a partition of N into connected subsets, that maximises the sum of the components‚Äô values. This problem is generally NP‚Äìcomplete; in particular, it is hard for a deÔ¨Åned class of valuation functions which are independent of disconnected members‚Äîthat is, two nodes have no effect on each other‚Äôs marginal contribution to their vertex separator. Nonetheless, for all such functions we provide bounds on the complexity of coalition structure generation over general and minor free graphs. Our proof is constructive and yields algorithms for solving corresponding instances of the problem. Furthermore, we derive polynomial time bounds for acyclic, K2,3 and K4 minor free graphs. However, as we show, the problem remains NP‚Äìcomplete for planar graphs, and hence, for any Kk minor free graphs where k ‚â• 5. Moreover, our hardness result holds for a particular subclass of valuation functions, termed edge sum, where the value of each subset of nodes is simply determined by the sum of given weights of the edges in the induced subgraph.},
	language = {en},
	author = {Voice, Thomas D and Polukarov, Maria and Jennings, Nicholas R},
